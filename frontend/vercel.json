{
	"headers": [
		{
			"source": "/(.*)",
			"headers": [
				{
					"key": "Strict-Transport-Security",
					"value": "max-age=31536000;"
				},
				{
					"key": "X-XSS-Protection",
					"value": "1; mode=block"
				},
				{
					"key": "X-Frame-Options",
					"value": "DENY"
				},
				{
					"key": "Content-Security-Policy",
					"value": "default-src 'self'; script-src 'self' 'unsafe-inline' 'unsafe-eval'; img-src 'self' data:;"
				},
				{
					"key": "Feature-Policy",
					"value": "microphone 'none'; camera 'none'; geolocation 'none'; payment 'none';"
				}
			]
		}
	],
	"rewrites": [
		{
			"source": "/(.*)",
			"destination": "/"
		}
	]
}